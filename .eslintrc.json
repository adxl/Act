{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "valid-typeof": 0,
        "no-console": 0,
        "import/extensions": 0,
        "import/no-absolute-path": 0,
        "import/no-unresolved": 0
    }
}